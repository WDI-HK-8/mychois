app.controller('GroceryListUpdateCtrl', ['$scope', '$http', '$auth', '$location', function($scope, $http, $auth, $location){
  $scope.updateList = {}
  $scope.updateList.list_id

  
  $scope.updateList.update = function() {
    $http.patch('/api/v1/grocery_lists/' + $scope.updateList.list_id, $scope.updateList).
    then(function(response){
      $scope.grocery_lists = response;
      console.log(response);
    }).
    catch(function(response){
      console.log(response);
    });
  }

}]);
